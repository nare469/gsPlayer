{
  "manifest_version": 2,

  "name": "Grooveshark",
  "description": "Player",
  "version": "1.0",
  "background": {
        "scripts": ["backgrounnd.js"]
    },
  "browser_action": {
    "default_title": "Groovershark",
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["tabs"]
}
